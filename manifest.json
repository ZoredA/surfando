{

  "manifest_version": 2,
  "name": "Surfando",
  "version": "1.0",

  "description": "Plays music that changes according to the website you are on!",

  "icons": {
    "48": "icons/queue_music_google.svg"
  },

  "options_ui": {
		"page": "options.html"
   },	

	"permissions": 
        [
            "storage", "activeTab", "webRequest", "alarms",
            "*://upload.wikimedia.org/*",
            "*://p.scdn.co/*",
            "https://api.spotify.com/*",
            "*://fonts.gstatic.com/*",
            "tabs"
        ],
    
    "browser_action":{
        "default_icon":"icons/queue_music_google.svg",
        "default_title": "Surfando",
        "default_popup": "popup/player.html"
    },
    "web_accessible_resources": [
        
    ],
    
    "background": {
      "page": "background.html"
    }
    
}
